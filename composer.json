    "keywords": [
        "doctrine", "dbal", "mysql", "upsert", "insert", "multiple"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Claudio Bizzotto"
    }],
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "codeception/codeception": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Pharako\\DBAL\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
